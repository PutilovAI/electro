@font-face {
    font-family: 'magistral_regular';
    src: url('../fonts/magistral.eot');
    src: url('../fonts/magistral.eot?#iefix') format('embedded-opentype'),
         url('../fonts/magistral.woff') format('woff'),
         url('../fonts/magistral.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

